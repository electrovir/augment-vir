{
    "name": "augment-vir",
    "version": "3.0.6",
    "homepage": "https://github.com/electrovir/augment-vir",
    "bugs": {
        "url": "https://github.com/electrovir/augment-vir/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/electrovir/augment-vir"
    },
    "license": "MIT",
    "author": {
        "name": "electrovir",
        "url": "https://github.com/electrovir"
    },
    "main": "dist/cjs/index.js",
    "module": "dist/esm/index.js",
    "types": "dist/types/index.d.ts",
    "scripts": {
        "build": "virmator frontend build",
        "compile": "rm -rf dist && tsc --project tsconfig.json && tsc --project tsconfig.cjs.json && tsc --project tsconfig.esm.json",
        "docs:update": "virmator code-in-markdown",
        "format": "virmator format",
        "prepublishOnly": "npm run compile && npm run test:all",
        "preview": "virmator frontend preview",
        "spellcheck": "virmator spellcheck",
        "start": "npm install && virmator frontend",
        "test": "virmator test",
        "test:all": "npm run test:types && npm test && npm run spellcheck && npm run test:format",
        "test:format": "virmator format check",
        "test:types": "tsc --noEmit",
        "test:web": "virmator test-web"
    },
    "dependencies": {
        "@type-challenges/utils": "^0.1.1",
        "@types/chai-as-promised": "^7.1.5",
        "chai-as-promised": "^7.1.1",
        "fs-extra": "10.1.0"
    },
    "devDependencies": {
        "@istanbuljs/nyc-config-typescript": "^1.0.2",
        "@types/chai": "^4.3.3",
        "@types/fs-extra": "9.0.13",
        "@types/mocha": "^10.0.0",
        "@types/node": "18.11.0",
        "chai": "^4.3.6",
        "cspell": "^6.12.0",
        "istanbul-smart-text-reporter": "^0.0.1",
        "mocha": "^10.1.0",
        "mocha-spec-reporter-with-file-names": "^0.0.0",
        "nyc": "^15.1.0",
        "prettier": "^2.7.1",
        "prettier-plugin-jsdoc": "^0.4.2",
        "prettier-plugin-multiline-arrays": "^1.1.1",
        "prettier-plugin-organize-imports": "^3.1.1",
        "prettier-plugin-packagejson": "^2.3.0",
        "prettier-plugin-sort-json": "^0.0.3",
        "prettier-plugin-toml": "^0.3.1",
        "ts-node": "^10.9.1",
        "typescript": "4.8.4",
        "virmator": "4.2.10"
    }
}
